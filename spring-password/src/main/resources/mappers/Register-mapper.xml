<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
        
<mapper namespace="com.kh.mapper.BCRegisterMapper">
	<insert id="insertBCRegister">
		<!-- NaverUser Table 에 네이버에서 가져온 값과 추가할 값을 작성해서 저장 -->
		INSERT INTO naver_user(id, email, username, password)
		VALUES (#{id}, #{email}, #{username}, #{password})
	</insert>
</mapper>